#include<iostream>
using namespace std;
int main()
{int sub1,sub2,sub3,sub4,sub5;
int sum;
float percentage;
cout<<"enter marks of five subjects";
cin>>sub1>>sub2>>sub3>>sub4>>sub5>>;
sum=sub1+sub2+sub3+sub4+sub5;
percentage=(sum/5);
cout<<"total obtained marks"<<sum<<endl<<"obtained percentage"<<percentage;
return 0;
}
